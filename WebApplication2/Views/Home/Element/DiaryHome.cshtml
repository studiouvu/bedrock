@{
    Layout = null;
}

<div style="width: 100%;">
    <div style="margin-left: auto; margin-right: auto;">
        <div style="max-width: 100%;">
            <div class="ob-box click-color" onclick='ReceiveCreateNewDiary()' style="width:100%; margin-left: auto; max-width: 150px; cursor: pointer; padding: 6px; background-color:#1f1f1f;">
                <div style="width:100%; height:100%; align-items: center;">
                    <div style="width:100%; height:100%; display: flex;">
                        <div class="hover-container" style="display: flex; width:100%; border: none; outline: none;">
                            <div class="text-center" style="width: 100%; align-items: center;">
                                +
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <div style='min-height:1px; margin-top: 15px; margin-bottom: 15px; width:100%;'></div>

        @foreach (var item in ViewBag.DiaryList)
        {
            <div id='@item' style="max-width: 100%;">
                <div class="ob-box click-color" onclick='' style="width:100%; cursor: pointer; padding: 5px;">
                    <div style="width:100%; height:100%; align-items: center;">
                        <div style="width:100%; height:100%; display: flex;">
                            <div class="hover-container" style="display: flex; width:100%; border: none; outline: none;">
                                @item
                                <div class="hover click-color unselectable" style="border-radius: 5px; cursor: pointer; margin-left:auto; padding-right: 6px; padding-left: 6px;">
                                    <font color="#6c6c6c">
                                        작성 24.11.14
                                    </font>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div style='min-height:1px; width:100%; background-color:#1f1f1f;'></div>
        }
    </div>
</div>